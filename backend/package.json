    "@google-cloud/secret-manager": "^5.0.0",
    "@google-cloud/speech": "^6.1.0",
    "@google-cloud/storage": "^7.0.0",
    "@google-cloud/vertexai": "^1.9.0",
    "@mux/mux-node": "^12.8.0",
