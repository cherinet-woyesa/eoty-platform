{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node scripts/auto-migrate.js; node server.js",
    "dev": "nodemon server.js",
    "populate-ai": "node scripts/populatePinecone.js",
    "migrate": "npx knex migrate:latest --env development",
    "migrate:rollback": "npx knex migrate:rollback --env development",
    "seed": "npx knex seed:run --env development",
    "init-db": "node scripts/init-database.js",
    "verify": "node ../verify-deployment.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/cloudfront-signer": "^3.921.0",
    "@aws-sdk/s3-request-presigner": "^3.922.0",
    "@mux/mux-node": "^12.8.0",
    "@pinecone-database/pinecone": "^2.2.2",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "langchain": "^0.0.206",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.9.7",
    "openai": "^4.104.0",
    "pg": "^8.11.3",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/migrations/",
      "/seeds/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ]
  }
}
